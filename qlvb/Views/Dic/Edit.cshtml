@model qlvb.Models.dic_normal

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_LayoutAdmin2.cshtml";
}
<div id="tf-services" style="width:100%;padding-top:100px;">
<div class="container">
<h2>Sửa từ</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
       

        <div class="editor-label">
            Nội dung
        </div>
        <div class="editor-field">
            <input type="text" name="word" id="word" value="@Model.word" style="width:300px;">
        </div>

        

        <p>
            <input type="submit" value="Sửa" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
</div>
</div>
